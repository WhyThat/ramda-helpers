{
  "name": "ramda-helpers",
  "version": "1.0.1",
  "description": "Some usefull helper make with ramda",
  "main": "ramda-helpers.js",
  "module": "ramda-helpers.es6",
  "jsnext:main": "dist/ramda-helpers.mjs",
  "browser": "main.browser.js",
  "scripts": {
    "prebuild": "eslint lib test",
    "build": "rollup -c",
    "doc": "jsdoc -c node_modules/ink-docstrap/template/jsdoc.conf.json -t node_modules/ink-docstrap/template -R README.md -r lib/ --verbose",
    "watch": "rollup -c -w",
    "pretest": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WhyThat/ramda-helpers.git"
  },
  "keywords": [
    "ramda",
    "library",
    "helpers"
  ],
  "author": "HATON Mathieu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WhyThat/ramda-helpers/issues"
  },
  "homepage": "https://github.com/WhyThat/ramda-helpers#readme",
  "dependencies": {
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.18.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "ink-docstrap": "^1.3.0",
    "jest": "^18.1.0",
    "jsdoc": "^3.4.3",
    "rollup": "^0.37.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0"
  },
  "jest": {
    "testRegex": "(.test.js$|/test/index.js$)",
    "coveragePathIgnorePatterns": [
      "/test/",
      "/config/",
      "/dist/",
      "/node_modules/"
    ]
  }
}
